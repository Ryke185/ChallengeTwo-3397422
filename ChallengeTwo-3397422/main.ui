from typing import List
import bisect

class SearchSuggestionSystem:
    def _init_(self, products: List[str]):
        # Sort products lexicographically for efficient lookup
        self.products = sorted(products)

    def getSuggestions(self, searchWord: str) -> List[List[str]]:
        result = []
        prefix = ""
        for char in searchWord:
            prefix += char
            # Use binary search to find the start index of matching prefix
            start = bisect.bisect_left(self.products, prefix)
            suggestions = []
            # Collect up to 3 suggestions that start with the current prefix
            for i in range(start, min(start + 3, len(self.products))):
                if self.products[i].startswith(prefix):
                    suggestions.append(self.products[i])
                else:
                    break
            result.append(suggestions)
        return result